super + Return
    $TERMINAL

super + e
    $TERMINAL -e neomutt

super + w
    $BROWSER

super + Home
    maim $HOME/Pictures/$(date +%s).png

super + F9
    sndioctl output.level=!

super + F10
    sndioctl output.level=-0.1

super + F11
    sndioctl output.level=+0.1

super + p
    $HOME/.local/bin/passmenu

super + o
    $HOME/.local/bin/passmenu
